                }
              } else if (data.Result == null && i == 2) {
                alert(
                  "Error occoured in your program : \n" +
                    JSON.stringify(data.Errors)
                );
                for (var k = 0; k <= 2; k++) {
                  var j = i + 1;
                  document.getElementById("t" + parseInt(j)).style.color =
                    "red";
                  document.getElementById("t" + parseInt(j)).innerHTML =
                    "Test Case " + j + "  \n Failed";
                }
              } else {
                var j = i + 1;
                document.getElementById("t" + parseInt(j)).style.color = "red";
                document.getElementById("t" + parseInt(j)).innerHTML =
                  "Test Case " + j + "  \n Failed";
              }
            })
            .fail(function (data, err) {
              alert(
                "Error occoured :  " +
                  JSON.stringify(data) +
                  " " +
                  JSON.stringify(err)
              );
            });
        }
      }
